<h1>New Bookmark</h1>
 
 <div class="row">
   <div class="col-md-8">
     <%= form_for [@topic, @bookmark], url: topic_bookmarks_path do |f| %>
     <%#= form_for [@post, @summary], url: post_summaries_path do |f| %>
       <div class="form-group">
         <%= f.label :url %>
         <%= f.text_field :url, class: 'form-control', placeholder: "Enter complete url" %>
       </div>
       <div class="form-group">
         <%= f.submit "Save", class: 'btn btn-success' %>
       </div>
     <% end %>
   </div>
 </div>